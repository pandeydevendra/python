    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = [x_p for x_p in range(-3,4)]\n",
    "y = [(x_p)**2 + 2*(x_p) -2 for x_p in x]\n",
    "print(x,\"\\n\", y)\n",
    "plt.plot(x, y)\n",
    "plt.xlabel(\"x-->\")\n",
    "plt.ylabel(\"y-->\")\n",
    "plt.title(\"polynomial\")\n",
    "plt.show()\n"
   ]
